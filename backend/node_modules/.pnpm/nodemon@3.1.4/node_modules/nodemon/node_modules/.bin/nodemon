#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/React/ReactGfg/movie-app/backend/node_modules/.pnpm/nodemon@3.1.4/node_modules/nodemon/bin/node_modules:/mnt/e/React/ReactGfg/movie-app/backend/node_modules/.pnpm/nodemon@3.1.4/node_modules/nodemon/node_modules:/mnt/e/React/ReactGfg/movie-app/backend/node_modules/.pnpm/nodemon@3.1.4/node_modules:/mnt/e/React/ReactGfg/movie-app/backend/node_modules/.pnpm/node_modules:/mnt/e/React/ReactGfg/movie-app/backend/node_modules:/mnt/e/React/ReactGfg/movie-app/node_modules:/mnt/e/React/ReactGfg/node_modules:/mnt/e/React/node_modules:/mnt/e/node_modules:/mnt/e/react/ReactGfg/movie-app/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/React/ReactGfg/movie-app/backend/node_modules/.pnpm/nodemon@3.1.4/node_modules/nodemon/bin/node_modules:/mnt/e/React/ReactGfg/movie-app/backend/node_modules/.pnpm/nodemon@3.1.4/node_modules/nodemon/node_modules:/mnt/e/React/ReactGfg/movie-app/backend/node_modules/.pnpm/nodemon@3.1.4/node_modules:/mnt/e/React/ReactGfg/movie-app/backend/node_modules/.pnpm/node_modules:/mnt/e/React/ReactGfg/movie-app/backend/node_modules:/mnt/e/React/ReactGfg/movie-app/node_modules:/mnt/e/React/ReactGfg/node_modules:/mnt/e/React/node_modules:/mnt/e/node_modules:/mnt/e/react/ReactGfg/movie-app/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nodemon.js" "$@"
else
  exec node  "$basedir/../../bin/nodemon.js" "$@"
fi
